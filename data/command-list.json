{
  "categories": {
    "general": "General",
    "dm": "Direct Message",
    "admin": "Administration"
  },
  "commands": {
  
    "stats": {
      "args": {
        "player": {
          "required": true,
          "description": "The player name or id of the user"
        },
        "platform": {
          "required": false,
          "description": "The platform to request",
          "options": [
            "xbox",
            "ps4",
            "switch",
            "pc"
          ]
        }
      },
      "desription": "Get general stats of a player.",
      "category": "general",
      "examples": [
        "stats ThatDPSChick ps4",
        "stats Zencep",
        "stats id:9152847"
      ]
    },
	
	"current": {
      "args": {
        "player": {
          "required": true,
          "description": "The player name or id of the user"
        }
      },
      "desription": "Get info about your current match.",
      "category": "general",
      "examples": [
        "current ThatDPSChick ps4",
        "current Zencep",
        "current id:9152847"
      ]
    },
	
	"match": {
      "args": {
        "matchID": {
          "required": true,
          "description": "The ID of the match."
        },
		"player": {
			"required": false,
			"description": "The player name or id of the user"
		},
      },
      "desription": "Get information about a match from the match ID.",
      "category": "general",
      "examples": [
        "match 963121647",
		"match 963121647 HelpImSuffering"
      ]
    },
	
	"history": {
      "args": {
