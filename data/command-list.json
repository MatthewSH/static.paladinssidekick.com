      "description": "Get general stats of a player.",
      "category": "general",
      "args": {
        "player": {
          "required": true,
          "description": "The player name or id of the player."
        },
        "platform": {
          "required": false,
          "description": "The platform to request",
          "options": [
            "xbox",
            "ps4",
            "switch",
            "pc"
          ]
        }
      },
      "examples": [
        "stats ThatDPSChick ps4",
        "stats Zencep",
        "stats id:9152847"
      ]
